{
  "name": "citykey-microsite",
  "private": true,
  "dependencies": {
    "bootstrap-sass": "~3.3.5",
    "jQuery.mmenu": "~5.4.0"
  },
  "overrides": {
    "bootstrap-sass": {
      "main": [
        "assets/stylesheets/_bootstrap.scss",
        "assets/fonts/bootstrap/*",
        "assets/javascripts/bootstrap.js"
      ]
    },
    "jQuery.mmenu": {
      "main": [
        "src/core/css/jquery.mmenu.scss",
        "src/core/js/jquery.mmenu.oncanvas.js",
        "src/addons/js/jquery.mmenu.offcanvas.js"
      ]
    }
  },
  "devDependencies": {
    "chai": "~3.2.0",
    "mocha": "~2.2.5"
  }
}
